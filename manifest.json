{
  "manifest_version": 3,
  "name": "17Lands Japanese Replacer",
  "version": "1.0",
  "web_accessible_resources": [
    {
      "resources": [
        "cardImageMap.json",
        "cardNameMap.json"
      ],
      "matches": [
        "https://www.17lands.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.17lands.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
